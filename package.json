{
  "name": "infer-dos",
  "version": "1.0.0",
  "description": "a helper tool to infer the Durable Objects of a given file",
  "bin": {
    "infer-dos": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js",
      "require": "./dist/cli.js"
    }
  },
  "scripts": {
    "check": "biome check && tsc && vitest --watch false",
    "build": "npx tsc src/* --outdir dist --module commonjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@cloudflare/actors": "^0.0.1-beta.0",
    "@cloudflare/workers-types": "^4.20250705.0",
    "@types/node": "^24.0.10",
    "agents": "^0.0.101",
    "partyserver": "^0.0.72",
    "tsup": "^8.5.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.23.0"
  },
  "dependencies": {
    "typescript": "^5.8.2"
  }
}
